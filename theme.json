  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--public-sans)",
      "fontSize": "var(--wp--preset--font-size-normal)",
      "fontWeight": "400",
      "lineHeight": "1.5"
    },
    "elements": {
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--heading)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--arimo)",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--ultra-large)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--super-large)",
          "fontWeight": "600",
          "lineHeight": "1.2"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--very-large)",
          "fontWeight": "500",
          "lineHeight": "1.2"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium-large)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--slightly-large)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--slightly-small)"
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--light)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family-arimo)",
          "textTransform": "uppercase"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--secondary)",
            "text": "var(--wp--preset--color--light)"
          }
        },
        "border": {
          "radius": "100px",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "bottom": "14px",
            "left": "26px",
            "right": "26px",
            "top": "14px"
          }
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--foreground)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary)"
          }
        },
        "typography": {
          "textDecoration": "none",
          "fontFamily": "var(--wp--preset--font-family--public-sans)"
        }
      }
    }
  },
